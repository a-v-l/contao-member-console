{
    "name": "a-v-l/contao-member-console",
    "description": "Add member command to contao-console for Contao Open Source CMS",
    "keywords": ["contao", "console"],
    "homepage": "https://github.com/a-v-l/contao-member-console",
    "type": "contao-bundle",
    "license": "MIT",
    "authors": [{
        "name": "Arndt v. Lucadou",
        "homepage": "https://www.lucadou.net"
    }],
    "support": {
        "issues": "https://github.com/a-v-l/contao-member-console/issues",
        "source": "https://github.com/a-v-l/contao-member-console",
        "forum": "https://community.contao.org"
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "contao/core-bundle": "^4.9 || ^5.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "contao/easy-coding-standard": "^3.0"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
            "AVL\\MemberConsole\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.3.x-dev"
        },
        "contao-manager-plugin": "AVL\\MemberConsole\\ContaoManager\\Plugin"
    },
    "scripts": {
        "cs-fixer": [
            "vendor/bin/ecs check src/ --fix --ansi"
        ]
    }
}
